{
  "kind": "collectionType",
  "collectionName": "students",
  "info": {
    "singularName": "student",
    "pluralName": "students",
    "displayName": "Student",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Male",
        "Female"
      ]
    },
    "email": {
      "type": "email",
      "required": false,
      "unique": true
    },
    "level": {
      "type": "enumeration",
      "enum": [
        "Level 100",
        "Level 200",
        "Level 300",
        "Level 400",
        "Level 500"
      ]
    },
    "department": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::department.department"
    },
    "courses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course.course"
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "nationality": {
      "type": "string"
    },
    "address": {
      "type": "text"
    },
    "phone": {
      "type": "string"
    },
    "middleName": {
      "type": "string"
    },
    "uid": {
      "type": "string"
    }
  }
}
