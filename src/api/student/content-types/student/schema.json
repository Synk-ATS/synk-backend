{
  "kind": "collectionType",
  "collectionName": "students",
  "info": {
    "singularName": "student",
    "pluralName": "students",
    "displayName": "Student"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Male",
        "Female"
      ]
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "password": {
      "type": "password",
      "required": true,
      "minLength": 8,
      "private": true
    },
    "level": {
      "type": "enumeration",
      "enum": [
        "Level 100",
        "Level 200",
        "Level 300",
        "Level 400",
        "Level 500"
      ]
    },
    "department": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::department.department"
    },
    "courses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course.course"
    },
    "avatar": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
