{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "course_code": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text"
    },
    "faculty": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::faculty.faculty"
    },
    "teachers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::teacher.teacher"
    }
  }
}
